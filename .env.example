# Arquivo .env.example para o projeto renne-plus
# Copie este arquivo para .env e preencha com suas configurações

# Configurações do PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=renneplus

# Configurações do Backend
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/renneplus?schema=public
JWT_SECRET=sua_chave_secreta_muito_segura_aqui
JWT_EXPIRES_IN=7d
PORT=3001
NODE_ENV=development

# Configurações do Mercado Pago
MERCADOPAGO_ACCESS_TOKEN=TEST-0000000000000000-000000-00000000000000000000000000000000-000000000

# Configurações do Frontend
NEXT_PUBLIC_API_URL=http://localhost:3001
